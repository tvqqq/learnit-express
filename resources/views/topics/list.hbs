<a href="/topics/add" class="btn btn-primary" role="button">Add</a>

<h1 class="mt-3">List Topics</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Status</th>
      <th scope="col">Description</th>
      <th scope="col">URL</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    
    {{#each topics}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.title}}</td>
      <td>{{this.status}}</td>
      <td>{{this.description}}</td>
      <td>{{this.url}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
